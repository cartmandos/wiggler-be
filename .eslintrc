{
  "root": true,
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:n/recommended",
    "prettier",
    "plugin:security/recommended-legacy"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "overrides": [],
  "rules": {
    "n/no-process-exit": "warn",
    "no-multi-spaces": "error",
    "indent": ["warn", 2, { "SwitchCase": 1 }],
    "no-unused-vars": "warn"
  }
}
